import java.util.Scanner; 
public class Faktöiyel {
	public static void main(String[] args){
		int sonuc=1,i=1;
		Scanner s1=new Scanner(System.in);
		System.out.println("Değeri giriniz:");
		int değer=s1.nextInt();
		while(değer>=i){
	sonuc=sonuc*i;
	i++;
	}
System.out.println("sonuc="+sonuc);

}
}
